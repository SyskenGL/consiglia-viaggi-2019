<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <Pane prefHeight="360.0" prefWidth="940.0" styleClass="content-pane">
        <children>
            <TextArea fx:id="textAreaReviewDescription" editable="false" focusTraversable="false" layoutX="14.0" layoutY="149.0" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="133.0" prefWidth="450.0" promptText="Review Description" styleClass="vertical-text-area" wrapText="true" />
            <TextArea fx:id="textAreaReviewTitle" editable="false" focusTraversable="false" layoutX="14.0" layoutY="66.0" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="450.0" promptText="Review Title" styleClass="horizontal-text-area" />
            <Button fx:id="buttonApprove" layoutX="14.0" layoutY="304.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="35.0" prefWidth="160.0" styleClass="button-approve" text="APPROVE" textAlignment="CENTER" translateX="50.0">
                <graphic>
                    <ImageView fitHeight="20.0" fitWidth="20.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true" translateX="-22.0">
                        <image>
                            <Image url="@../icons/ic_approve.png" />
                        </image>
                    </ImageView>
                </graphic>
                <cursor>
                    <Cursor fx:constant="HAND" />
                </cursor>
            </Button>
            <Button fx:id="buttonDisapprove" layoutX="304.0" layoutY="304.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="35.0" prefWidth="160.0" styleClass="button-disapprove" text="DISAPPROVE" textAlignment="CENTER" translateX="-50.0">
                <graphic>
                    <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true" translateX="-10.0">
                        <image>
                            <Image url="@../icons/ic_disapprove.png" />
                        </image>
                    </ImageView>
                </graphic>
                <cursor>
                    <Cursor fx:constant="HAND" />
                </cursor>
            </Button>
            <HBox alignment="CENTER" layoutX="515.0" layoutY="66.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="235.0" prefWidth="370.0" styleClass="image-slider">
                <children>
                    <ImageView fx:id="imageSlider" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@../icons/ic_ghost.png" />
                        </image></ImageView>
                </children></HBox>
            <Label layoutX="14.0" prefHeight="67.0" styleClass="label-section-name" text="Reviews Approval Panel" />
            <RadioButton fx:id="radioButtonAutopilot" contentDisplay="CENTER" graphicTextGap="10.0" layoutX="800.0" layoutY="22.0" mnemonicParsing="false" prefHeight="24.0" styleClass="autopilot-radio-button" text="Enable autopilot" translateX="-3.0">
                <cursor>
                    <Cursor fx:constant="HAND" />
                </cursor>
                <font>
                    <Font size="14.0" />
                </font>
            </RadioButton>
            <HBox alignment="CENTER" layoutX="515.0" layoutY="301.0" prefHeight="30.0" prefWidth="370.0" spacing="5.0" translateY="12.0">
                <children>
                    <Button fx:id="buttonPreviousImage" mnemonicParsing="false" styleClass="button-image">
                        <graphic>
                            <ImageView fx:id="iconButtonPreviousImage" fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@../icons/ic_arrow_left_default.png" />
                                </image>
                            </ImageView>
                        </graphic>
                        <cursor>
                            <Cursor fx:constant="HAND" />
                        </cursor></Button>
                    <Label fx:id="labelCurrentImageCount" alignment="CENTER" contentDisplay="CENTER" prefHeight="24.0" styleClass="label-image-slider-count" text="0 - 0" />
                    <Button fx:id="buttonNextImage" contentDisplay="RIGHT" mnemonicParsing="false" styleClass="button-image">
                        <graphic>
                            <ImageView fx:id="iconButtonNextImage" fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@../icons/ic_arrow_right_default.png" />
                                </image>
                            </ImageView>
                        </graphic>
                        <cursor>
                            <Cursor fx:constant="HAND" />
                        </cursor>
                    </Button>
                </children>
            </HBox>
        </children>
    </Pane>
</AnchorPane>
