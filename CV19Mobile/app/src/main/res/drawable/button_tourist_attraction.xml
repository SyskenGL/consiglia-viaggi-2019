<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="rectangle">
            <size
                android:height="100dp"
                android:width="100dp"/>
            <padding
                android:top="10dp"
                android:bottom="10dp"
                android:right="10dp"
                android:left="10dp"/>
            <corners
                android:radius="40dp" />
            <stroke
                android:width="7dp"
                android:color="@color/white"/>
            <padding
                android:left="12dp"
                android:top="12dp"
                android:right="12dp"
                android:bottom="12dp" />
        </shape>
    </item>

    <item>
        <shape
            android:shape="rectangle">
            <corners
                android:radius="30dp" />
            <size
                android:height="80dp"
                android:width="80dp"/>
            <solid
                android:color="@color/white"/>
        </shape>
    </item>

    <item
        android:drawable="@drawable/ic_tourist_attraction"
        android:bottom="18dp"
        android:left="18dp"
        android:right="18dp"
        android:top="18dp" />

</layer-list>